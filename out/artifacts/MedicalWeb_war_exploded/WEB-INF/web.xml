<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
        <welcome-file>medicalWeb.jsp</welcome-file>
        <welcome-file>dataEntry.jsp</welcome-file>
        <welcome-file>updateMedical.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>MedicalOne</servlet-name>
        <servlet-class>com.medical.app.util.TestServletOne</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MedicalOne</servlet-name>
        <url-pattern>/medicalval</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>MedicalThree</servlet-name>
        <servlet-class>com.medical.app.util.MedicalDataEntry</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MedicalThree</servlet-name>
        <url-pattern>/dataent</url-pattern>
    </servlet-mapping>
</web-app>